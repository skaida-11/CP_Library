<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cp_library.BookEdit">
    <children>

        <Label layoutX="82.0" layoutY="64.0" prefHeight="18.0" prefWidth="63.0" text="Author" />
        <TextField fx:id="editAuthorField" layoutX="82.0" layoutY="82.0" prefHeight="26.0" prefWidth="290.0" />
        <Label layoutX="82.0" layoutY="127.0" prefHeight="18.0" prefWidth="48.0" text="Title" />
        <TextField fx:id="editTitleField" layoutX="83.0" layoutY="153.0" prefWidth="290.0" />
        <Label layoutX="82.0" layoutY="200.0" prefHeight="18.0" prefWidth="63.0" text="ISBN" />
        <TextField fx:id="editIsbnField" layoutX="83.0" layoutY="231.0" prefWidth="290.0" />
        <Button fx:id="editButton" layoutX="420.0" layoutY="82.0" mnemonicParsing="false" onAction="#editBook" text="Edit" />
        <Label layoutX="82.0" layoutY="279.0" prefHeight="18.0" prefWidth="100.0" text="Available" />
        <ChoiceBox fx:id="available" layoutX="82.0" layoutY="310.0" prefWidth="150.0" />
        <Button layoutX="420.0" layoutY="153.0" mnemonicParsing="false" onAction="#backToBook" text="Back" />
        <Label fx:id="editMessage" layoutX="420.0" layoutY="230.0" prefHeight="26.0" prefWidth="165.0" />
    </children>
</AnchorPane>
